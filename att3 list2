programa {
  funcao inicio() {
    real n1, n2

    escreva("n1 = ")
    leia(n1)
    escreva("n2 = ")
    leia(n2)

    se (n1==n2){
      escreva("Números iguais.")
    }
    
    se (n1>n2){
      escreva("n1 é mior que n2.")
    }
    
    se (n1<n2){
      escreva("n2 é mior que n1.")
    }
  }
}

programa {
  funcao inicio() {
    real a, b, c

    escreva ("Digite o valor de A: ")
    leia(a)
    escreva ("Digite o valor de B: ")
    leia(b)
    escreva ("Digite o valor de C: ")
    leia(c)

    se (a<(b+c)){
      se (b<(c+a)) {
        se (c<(a+b)){
          escreva("É um triângulo")
        }
      }
    }
    senao {
      escreva("Não é um triângulo.")
    }



  }
}
